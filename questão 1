js (script.js):

let i = prompt("Digite sua primeira nota: ")
let ii = prompt("Digite sua segunda nota: ")
let iii = prompt("Digite ua terceira nota: ")

i = parseFloat(i)
ii = parseFloat(ii)
iii = parseFloat(iii)

media = (i+ii+iii)/3;
 
alert("Sua média é: "+media)
